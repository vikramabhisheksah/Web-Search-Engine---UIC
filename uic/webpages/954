            <script>
                function loadResources(tag, attr) {
                    var els = document.getElementsByTagName(tag);
                    for (var i = 0; i < els.length; i++) {
                        var el = els[i];
                        var val = el.getAttribute("data-" + attr);
                        if (val) {
                            el.setAttribute(attr, PageDataPayload.CdnBaseUrl + val);
                            el.removeAttribute("data-" + attr);
                        }
                    }
                }

                function loadAllResources() {
                    PageDataPayload.CdnBaseUrl =
                        PageDataPayload.CdnBaseUrl.replace("##ver##", PageDataPayload.CdnVersion || window.cdnVersion || "");

                    loadResources("link", "href");
                    loadResources("script", "src");
                };
            </script>

            <link rel="stylesheet" type="text/css" data-href="fonts/fonts.styles.css" />
