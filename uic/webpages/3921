            function checkboxOther(name_of_field) { 
                var elements = document.survey_1820274_1209T191507.elements[name_of_field]; var length = elements.length; 
                if(elements[length-1].checked) 
                { 
                    document.survey_1820274_1209T191507[name_of_field + '_other'].disabled=false; 
                    document.survey_1820274_1209T191507[name_of_field + '_other'].focus(); 
                } 
                else 
                { 
                    document.survey_1820274_1209T191507[name_of_field + '_other'].disabled=true; 
                    document.survey_1820274_1209T191507[name_of_field + '_other'].value=''; 
                } 
            } 
