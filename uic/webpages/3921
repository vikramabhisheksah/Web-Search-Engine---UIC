            function radioNotOther(name_of_field) {
                var name = document.survey_1820274_1209T191507[name_of_field + '_other'];
                if(typeof(name) != 'undefined' && name != null){
                    document.survey_1820274_1209T191507[name_of_field + '_other'].disabled=true; 
                    document.survey_1820274_1209T191507[name_of_field + '_other'].value=''; 
                }
            }
            
            function selectOther(name_of_field) {
                var elements = document.survey_1820274_1209T191507.elements[name_of_field]; 
