                      <h1 class="_title post-title">UIC Student Records Policy</h1>
              
        
        
              
    
        
  </div>
        <div class="components">

  
    
      

<section class="component  ">

  
  <div class="component-text-block">
  
    <div class="component-description">
                    <h2 id="uic-student-records-policy" class="__title invisible">UIC Student Records Policy</h2>
        
            </div>


  
  <h2>Page Sections:</h2>
<ul>
