            function validate(){
                if(document.survey_2689789_1209T191340.isValidate.value == 'true'){
                    document.survey_2689789_1209T191340.isValidate.value='false';
                    document.survey_2689789_1209T191340.USER_userId.value=document.survey_2689789_1209T191340.USER_userId_for_save.value;
