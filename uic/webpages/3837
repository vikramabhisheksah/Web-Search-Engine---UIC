            function checkboxOther(name_of_field) { 
                var elements = document.survey_2689789_1209T191340.elements[name_of_field]; var length = elements.length; 
                if(elements[length-1].checked) 
                { 
                    document.survey_2689789_1209T191340[name_of_field + '_other'].disabled=false; 
                    document.survey_2689789_1209T191340[name_of_field + '_other'].focus(); 
                } 
                else 
                { 
                    document.survey_2689789_1209T191340[name_of_field + '_other'].disabled=true; 
                    document.survey_2689789_1209T191340[name_of_field + '_other'].value=''; 
                } 
            } 
