                if(typeof(name) != 'undefined' && name != null){
                    document.survey_2689789_1209T191340[name_of_field + '_other'].disabled=true; 
                    document.survey_2689789_1209T191340[name_of_field + '_other'].value=''; 
                }
            }
            
