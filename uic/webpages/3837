			function checkMaxFileSize(inputId, inputIdNoLb, button, maxSizeDisplay) 
			{
				var iSize =  (document.getElementById(inputIdNoLb).files[0].size); 
				var maxSize = maxSizeDisplay * 1024 * 1024;
				if(iSize > maxSize)
				{
					alert('The file size is too large. The max file size is ' + maxSizeDisplay + ' M.  Please reduce the file size and try again.');
					document.getElementById(inputIdNoLb).value='';                 
					return false;
				}
				else
				{
					return true;
				}
			}
