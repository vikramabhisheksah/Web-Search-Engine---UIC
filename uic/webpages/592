      getPT: function(cb) {
        if (this.pt !== undefined) {
          cb(this.success, this.pt);
        } else {
          this.cbs.push(cb);
        }
      },
      setPT: function(s, pt) {
        this.success = s;
        this.pt = pt;
        if (this.cbs.length) {
          for (var i = 0; i < this.cbs.length; i++) {
            this.cbs[i](s, pt);
          }
        }
      }
    };
